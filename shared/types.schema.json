{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "AuthProps": {
            "additionalProperties": false,
            "properties": {
                "userPoolClientId": {
                    "type": "string"
                },
                "userPoolId": {
                    "type": "string"
                }
            },
            "required": [
                "userPoolClientId",
                "userPoolId"
            ],
            "type": "object"
        },
        "Game": {
            "additionalProperties": false,
            "properties": {
                "description": {
                    "type": "string"
                },
                "developer": {
                    "type": "string"
                },
                "genre": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "number"
                },
                "price": {
                    "type": "string"
                },
                "releaseDate": {
                    "type": "string"
                },
                "reviewCount": {
                    "type": "number"
                },
                "reviewPercentage": {
                    "type": "number"
                },
                "title": {
                    "type": "string"
                }
            },
            "required": [
                "description",
                "developer",
                "genre",
                "id",
                "price",
                "releaseDate",
                "reviewCount",
                "reviewPercentage",
                "title"
            ],
            "type": "object"
        },
        "SignUpTypes": {
            "additionalProperties": false,
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            },
            "required": [
                "email",
                "password",
                "username"
            ],
            "type": "object"
        }
    }
}

